{
  "name": "parallel-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\"  \"nodemon dist/index.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mikro-orm/cli": "^5.6.13",
    "@mikro-orm/core": "^5.6.13",
    "@mikro-orm/postgresql": "^5.6.13",
    "@mikro-orm/reflection": "^5.6.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "rimraf": "^4.3.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.21",
    "ts-morph": "^17.0.1"
  },
  "moduleAliases": {
    "@src/resources": "dist/resources",
    "@src/utils": "dist/utils",
    "@src/middleware": "dist/middleware"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
